<Window x:Class="ManifestationMapApp.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManifestationMapApp"
        mc:Ignorable="d"
         ResizeMode="NoResize"
        FontSize="22" FontWeight="Bold"
        WindowStartupLocation="CenterScreen"
        Title="Warning" Height="250" Width="505.219"
      >
    <Grid Background="#FF96D482">
        <TextBlock  Name="message" Margin="0 50" FontSize="18" FontFamily="Lato"  HorizontalAlignment="Center">
        </TextBlock>
        <Button  Content="Yes" Name="btnYes" Height="37" Background="Silver" FontSize="18"  Margin="30,0,314.2,15"   VerticalAlignment="Bottom" Click="BtnYes_Click" ></Button>
        <Button   Content="No" Name="btnNo" Height="36" Background="Silver" FontSize="18" Margin="278,0,40.2,15"   VerticalAlignment="Bottom" Click="BtnNo_Click" RenderTransformOrigin="0.5,0.5">
          
        </Button>
    </Grid>
</Window>
